import random

class Colour:
    EC = "\033[0m"
    RED = "\033[0;31m"
    ORANGE = "\033[0;33m"
    YELLOW = "\033[0;33;1m"
    GREEN = "\033[0;32m"
    BLUE = "\033[0;34;1m"
    INDIGO = "\033[0;35m"
    VIOLET = "\033[0;35;1m"

c = Colour
r = c.RED + "R"
a = c.ORANGE + "a"
i = c.YELLOW + "i"
n = c.BLUE + "n"
b = c.GREEN + "b"
o = c.INDIGO + "o"
w = c.RED + "w"
c2 = c.ORANGE + "C"
r2 = c.YELLOW + "r"
a2 = c.BLUE + "a"
w2 = c.GREEN + "w"
l2 = c.INDIGO + "l"
e2 = c.RED + "e"
r3 = c.ORANGE + "r"

print(r + a + i + n + b + o + w + " " + c2 + r2 + a2 + w2 + l2 + e2 + r3 + Colour.EC)
print()
while True:
    print("%sCrawling http://www.google.co.uk%s" % (c.RED, c.EC))
    print("%sResponse: 200 OK%s" % (c.YELLOW, c.EC))
    print("%sParsing HTML...%s" % (c.ORANGE, c.EC))
    rand = random.randint(2, 50)
    print("%sFound %s links. Created %s new threads.%s" % (c.GREEN, rand, rand-1, c.EC))
    print("%sINSERT INTO webpages%s" % (c.BLUE, c.EC))
    print("%sINSERT INTO words%s" % (c.INDIGO, c.EC))
    print("%sINSERT INTO webpages_words%s" % (c.VIOLET, c.EC))
    print()
